# vim:se syn=tcl:
#

use cc cc-shared

options {
}

set f [open settings.conf w]
foreach n [lsort [dict keys [all-defines]]] {
    set v [get-define $n]
    puts $f "set $n [list $v]"
}
close $f

if {![file exists Makefile]} {
    # Normally this would be autosetup/tmake
    catch {exec ../tmake --init}
}
