storage none
pageformat raw

accesslevel status

field type {
	stdmode hidden
	default hour
}

display {
	package require rrdgraph

	set r [rrdgraph env temp [cgi get type]]
	$r stdinit
	$r title "Temperature"
	$r ylabel "\xb0Celsius"
	$r line m1 m1temp -label "Modem 1" -format "%4.1lf" -units "\xb0Celsius"
	$r line rf1 rf1temp -label "RFU 1  " -format "%4.1lf" -units "\xb0Celsius"
	$r line m2 m2temp -label "Modem 2" -format "%4.1lf" -units "\xb0Celsius"
	$r line rf2 rf2temp -label "RFU 2  " -format "%4.1lf" -units "\xb0Celsius"
	$r output
	$r close
}
