storage none
pageformat raw

accesslevel status

field type {
	stdmode hidden
	default hour
}

display {
	package require rrdgraph

	set r [rrdgraph link errors [cgi get type]]
	$r stdinit
	$r params --logarithmic
	$r title "BER"
	$r ylabel "errs/sec"
	$r line m1 m1errs -label "Modem 1" -format "%7.1le" -units "errs/sec" -multiplier "2e-9"
	$r line m2 m2errs -label "Modem 2" -format "%7.1le" -units "errs/sec" -multiplier "2e-9"
	$r output
	$r close
}
